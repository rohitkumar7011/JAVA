import java.util.Scanner;

public class FACtorial {
    public static void main(String[] args) {
        System.out.println("enter the number:");
        int n = new Scanner(System.in).nextInt();
        int fact = 1;
        for(int i = n; i>0; i-- ){
            fact = fact * i;

        }
        System.out.println(fact);
    }
}
